{
  "expo": {
    "name": "Sportify",
    "slug": "fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/custom-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/custom-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/custom-adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["LOCATION"],
      "package": "com.ardhiqii.fe"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d01bbd69-4f41-49d5-9d4b-6e4efc715522"
      }
    }
  }
}
